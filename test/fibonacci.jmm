import io;

class Fibonacci {

    public static void main(String[] a) {
        Fibonacci fb;

        fb = new Fibonacci();
        fb.printSequence(15);
    }

    public int fibonacci(int n) {
        int toReturn;

        if(n < 0) {
            toReturn = 0-1;
        } else if (n < 2) {
            toReturn = 1;
        } else {
            toReturn = this.fibonacci(n - 1) + this.fibonacci(n - 2);
        }

        return toReturn;
    }

    public int printSequence(int n) {
        int count;
        count = 0;
        while (count < n) {
            io.println(this.fibonacci(count));
            count = count + 1;
        }
        return 0;
    }

}