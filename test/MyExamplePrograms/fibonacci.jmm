import io;

class Fibonacci {

    public static void main(String[] a) {
        Fibonacci fb;
        int fibVal;

        fb = new Fibonacci();
        fibVal = fb.fibonacci(15);
        io.println(fibVal);
    }

    public int fibonacci(int n, boolean noPrint) {
        int toReturn;

        if (n < 0) {
            toReturn = 0 - 1;
        } else if (n < 2) {
            toReturn = 1;
        } else {
            toReturn = this.fibonacci(n - 1, true) + this.fibonacci(n - 2, true);
        }

        return toReturn;
    }

    public int fibonacci(int n) {
        int fib1;
        int fib2;
        int toReturn;

        if (n < 0) {
            toReturn = 0 - 1;
            io.println(0 - 1);
        } else if (n < 2) {
            toReturn = 1;
            io.println(1);
        } else {
            fib1 = this.fibonacci(n - 1);
            io.println(fib1);
            fib2 = this.fibonacci(n - 2, true);
            toReturn = fib1 + fib2;
        }
        return toReturn;
    }
}