import io;

class C {
    int i;

    public int f() {
        int sum;
        int j;
        int[] A;
        boolean b;

        A = new int[10];
        i = 0;
        b = true;
        while (i < 10 && b) {
            A[i] = i;
            i = i + 1;
        }

        i = 0;
        sum = 0;
        while (i < 10 && b) {
            sum = sum + A[i];
            i = i + 1;
        }

        return sum * 0 + 6;
    }

    public static void main(String[] args) {
        io.println(new C().f());
    }
}
